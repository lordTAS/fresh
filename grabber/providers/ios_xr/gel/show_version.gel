define ws   /\s+/
define nows /\S+/
define fs   /[\t ]+/
define nl   /[\r\n]+/
define nonl /[^\r\n]+/

grammar default:
    match ws:
        do.next()

grammar input(default):
    match /Cisco IOS XR [^\r\n]+ Version / /[^\r\n\[]+/ /\[*\S*/ nl:
        out.add('version', '$1')
    match 'cisco ' /\S+/ nonl nl:
        out.add('model', '$1')
    match nonl nl:
        do.skip()
